{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false
    },
    "compileOnSave": true,
    "filesGlob": [
        "./**/*.ts",
        "!./*/**/*.d.ts",
        "!./omnisharp-client.d.ts",
        "!./omnisharp-server.d.ts",
        "!./typings/**/*.d.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "lib/candidate-finder.ts",
        "lib/clients/client-base.ts",
        "lib/clients/client-v1.ts",
        "lib/clients/client-v2.ts",
        "lib/clients/composite-client-base.ts",
        "lib/clients/composite-client-v1.ts",
        "lib/clients/composite-client-v2.ts",
        "lib/contexts.ts",
        "lib/decorators.ts",
        "lib/drivers/http.ts",
        "lib/drivers/stdio.ts",
        "lib/drivers/websocket.ts",
        "lib/enums.ts",
        "lib/omnisharp-client.ts",
        "lib/omnisharp-path.ts",
        "lib/options.ts",
        "lib/project-finder.ts",
        "lib/proxy/decorators.ts",
        "lib/proxy/proxy-client-base.ts",
        "lib/proxy/proxy-client-v1.ts",
        "lib/proxy/proxy-client-v2.ts",
        "lib/queue.ts",
        "lib/response-handling.ts",
        "lib/stdio/child.ts",
        "spec/candidate-find-spec.ts",
        "spec/omnisharp-client-commands-v2.ts",
        "spec/omnisharp-client-commands.ts",
        "spec/omnisharp-client-spec.ts",
        "spec/project-find-spec.ts",
        "spec/stdio-spec.ts",
        "tsd.d.ts"
    ],
    "exclude": []
}
